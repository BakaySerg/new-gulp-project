{
    "editor.fontSize": 15.2,
    "editor.tabSize": 3,
    "editor.multiCursorModifier": "ctrlCmd",
    "editor.letterSpacing": 0.2,
    "editor.renderLineHighlight": "none",
    "editor.links": false,
    "editor.minimap.maxColumn": 70,
    "editor.minimap.renderCharacters": false,
    "editor.overviewRulerBorder": false,
    "editor.selectionHighlight": false,
    "editor.smoothScrolling": true,
    "editor.matchBrackets": "never",
    "editor.cursorBlinking": "solid",
    "editor.highlightActiveIndentGuide": false,
    "editor.fontFamily": "Droid sans mono, Consolas,'Courier New', monospace",
    "editor.fontLigatures": true,
    "editor.codeLens": false,
    "editor.formatOnPaste": true,
    "editor.wordWrap": "off",
    "emmet.triggerExpansionOnTab": true,
    "files.trimTrailingWhitespace": true,
    "typescript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBrackets": true,
    "prettier.useTabs": true,
    "grunt.autoDetect": "off",
    "auto-rename-tag.activationOnLanguage": ["html","xml","php","vue","javascript"],
    "editor.fastScrollSensitivity": 7,
    "bracketPairColorizer.showHorizontalScopeLine": false,
    "bracketPairColorizer.showVerticalScopeLine": false,
    "terminal.integrated.lineHeight": 0,
    "vetur.format.options.tabSize": 3,
    // "vetur.completion.useScaffoldSnippets": false,
    // "eslint.validate": [
    //     {
    //         "language": "vue",
    //         "autoFix": true
    //     },
    //     {
    //         "language": "html",
    //         "autoFix": true
    //     },
    //     {
    //         "language": "javascript",
    //         "autoFix": true
    //     }
    // ],
    // "eslint.autoFixOnSave": true,
    "prettier.tabWidth": 3,
    "files.autoSaveDelay": 9000,
    "explorer.confirmDelete": false,
    "files.autoSave": "onFocusChange",
    "workbench.editor.enablePreview": false,
    "workbench.tree.indent": 9,
    "terminal.integrated.fontSize": 13,
    "explorer.confirmDragAndDrop": false,
    "editor.tokenColorCustomizations": {
        "textMateRules": [
            {
                "scope": [ "support", "support.class", "support.constant", "support.function", "support.other", "support.type", "support.type.property-name", "support.variable" ],
                "settings": { "fontStyle": "" }
            }
        ]
    },
    "breadcrumbs.enabled": false,
    "workbench.iconTheme": "vscode-icons",
    "workbench.colorTheme": "Monokai",
    "workbench.startupEditor": "newUntitledFile",
    "git.confirmSync": false,
    "vsicons.presets.foldersAllDefaultIcon": true,
    "terminal.integrated.cursorStyle": "line",
    "[css]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "vsicons.dontShowNewVersionMessage": true,
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": true
    },
    "explorer.autoReveal": false,
    "security.workspace.trust.untrustedFiles": "open",
    "editor.minimap.enabled": false,
    "editor.scrollbar.verticalScrollbarSize": 7,
    "editor.scrollbar.horizontalScrollbarSize": 7,
    "emmet.showSuggestionsAsSnippets": true,
    "html.hover.documentation": false,
    "html.hover.references": false,
    "scss.hover.references": false,
    "scss.hover.documentation": false,
    "workbench.editor.decorations.colors":false,
    "workbench.editor.decorations.badges": false,
    "editor.autoIndent": "advanced",
    "terminal.integrated.defaultProfile.windows": "Git Bash",
    "[html]": {
        "editor.defaultFormatter": "vscode.html-language-features"
},
"editor.glyphMargin": false,
"editor.parameterHints.enabled": false,
"editor.hover.enabled": false,
"editor.lineHeight": 1.4,
"editor.insertSpaces": false,
"editor.linkedEditing": true,
"ecsstractor_port.add_comment": false,
"ecsstractor_port.empty_line_before_nested_selector": false,
"bracketPairColorizer.depreciation-notice": false,
"prettier.printWidth": 300,
"prettier.proseWrap": "never",

    // "editor.tokenColorCustomizations": {
    //     "comments": "#5a5a5a"
    // }
}